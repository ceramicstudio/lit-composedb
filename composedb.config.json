{
  "anchor": {},
  "http-api": {
    "cors-allowed-origins": [".*"],
    "admin-dids": ["did:key:<your admin did>"]
  },
  "ipfs": { "mode": "bundled" },
  "logger": { "log-level": 2, "log-to-files": false },
  "metrics": { "metrics-exporter-enabled": false, "metrics-port": 9090 },
  "network": { "name": "testnet-clay" },
  "node": {},
  "state-store": { "mode": "fs", "local-directory": "~/.ceramic/statestore/" },
  "indexing": {
    "db": "postgres://postgres:mysecretpassword@localhost:5432/postgres",
    "allow-queries-before-historical-sync": true,
    "models": []
  }
}
